{
  "manifest_version": 2,

  "name": "CheckSteemitLink",
  "description": "This extension which denotes the phising link in Steemit",
  "version": "1.2",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "content_scripts": [{
      "matches": ["<all_urls>"],
      "js":[
          "contentscript.js"
      ]
  }]
}
